{
	"version": "0.2.0",
	"configurations": [
    {
			"name": "LaunchSvc",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/Service/app.js",
			"stopOnEntry": true,
			"args": [],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "externalTerminal",
			"sourceMaps": false
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"sourceMaps": false,
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		}
	]
}